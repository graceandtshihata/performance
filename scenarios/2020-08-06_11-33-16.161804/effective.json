   "class": "bzt.modules.blazemeter.ServiceStubCaptureHAR"
  },
  "final_stats": {
   "class": "bzt.modules.reporting.FinalStatus"
  }
 },
 "cli-aliases": {
  "cloud": {
   "provisioning": "cloud"
  },
  "detach": {
   "modules": {
    "cloud": {
     "detach": true
    }
   }
  },
  "func": {
   "modules": {
    "console": {
     "disable": true
    }
   },
   "settings": {
    "aggregator": "functional-consolidator"
   }
  },
  "gui": {
   "modules": {
    "jmeter": {
     "gui": true
    }
   }
  },
  "install-tools": {
   "services": [
    "install-checker"
   ]
  },
  "local": {
   "provisioning": "local"
  },
  "locations": {
   "modules": {
    "cloud": {
     "dump-locations": true
    }
   },
   "provisioning": "cloud"
  },
  "report": {
   "reporting": [
    "blazemeter"
   ]
  },
  "public": {
   "modules": {
    "blazemeter": {
     "public-report": true
    }
   }
  },
  "sequential": {
   "modules": {
    "local": {
     "sequential": true
    }
   }
  },
  "lint": {
   "cli": {
    "linter": {
     "lint-and-exit": true
    }
   }
  },
  "nolint": {
   "cli": {
    "linter": {
     "disable": true
    }
   }
  }
 },
 "cli": {
  "linter": {
   "disable": false,
   "warn-on-unfamiliar-fields": false,
   "ignored-warnings": [],
   "lint-and-exit": false
  }
 },
 "settings": {
  "aggregator": "consolidator",
  "default-executor": "jmeter",
  "env": {
   "OBJC_DISABLE_INITIALIZE_FORK_SAFETY": "YES",
   "TAURUS_ARTIFACTS_DIR": "/Users/grace.tshihata/pit/performance/scenarios/2020-08-06_11-33-16.161804"
  }
 },
 "provisioning": "local",
 "reporting": [
  {
   "module": "final-stats"
  },
  {
   "module": "console"
  },
  {
   "module": "junit-xml"
  }
 ],
 "services": [
  {
   "module": "monitoring",
   "local": [
    {
     "metrics": [
      "cpu",
      "mem",
      "bytes-sent",
      "bytes-recv",
      "disk-read",
      "disk-write",
      "disk-space",
      "engine-loop",
      "conn-all"
     ]
    }
   ]
  },
  {
   "module": "shellexec",
   "prepare": [
    {
     "command": "mvn -Dmaven.test.skip=true clean package"
    }
   ]
  }
 ],
 "execution": [
  {
   "executor": "junit",
   "concurrency": 500,
   "ramp-up": "15s",
   "scenario": "apiCall",
   "run-items": [
    "loadtests.ClientAPITest#post1"
   ]
  }
 ],
 "scenarios": {
  "apiCall": {
   "script": "./src/test/java/loadtests",
   "additional-classpath": [
    "target/",
    "target/LoadTestArtifacts/commons-io-2.6.jar",
