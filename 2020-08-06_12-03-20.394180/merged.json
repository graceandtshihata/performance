{
 "execution": [
  {
   "executor": "jmeter",
   "concurrency": 100,
   "ramp-up": "10s",
   "scenario": "apiCall",
   "run-items": [
    "loadtests.ClientAPITest#pre1"
   ]
  }
 ],
 "scenarios": {
  "apiCall": {
   "script": "./src/test/java/loadtests",
   "additional-classpath": [
    "target/*"
   ]
  }
 },
 "services": [
  {
   "module": "shellexec",
   "prepare": "mvn -Dmaven.test.skip=true  clean package"
  }
 ],
 "reporting": [
  {
   "module": "junit-xml"
  }
 ]
}
